<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gu_rl">
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/galgotias_university_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/galgotias_university_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SearchBarActivity"></activity>
        <activity android:name=".UserDetailsActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".OTPVerificarionActivity" />
        <activity android:name=".ForgetPassActivity" />
        <activity android:name=".ClassRoomsActivity" />
        <activity android:name=".FacultyActivity" />
        <activity android:name=".HigherAuthorityActivity" />
        <activity android:name=".DepartmentActivity" />
        <activity android:name=".BasicNeedActivity" />
        <activity android:name=".StudentLoginActivity" />
        <activity android:name=".StudentRegistrationActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>